#!/bin/sh

make


## Tests to find approximate runtime
echo 'Binary - 5050'
./main -v 1 -h binary -t 1 -s 5050 -p 338350

echo 'Binary - 63000'
./main -v 1 -h binary -t 1 -s 63000 -p 10542250

echo 'Binary - 125250'
./main -v 1 -h binary -t 1 -s 125250 -p 41791750

echo 'Binary - 251000'
./main -v 1 -h binary -t 1 -s 251000 -p 83834500

echo 'Binary - 500500'
./main -v 1 -h binary -t 1 -s 500500 -p 333833500

