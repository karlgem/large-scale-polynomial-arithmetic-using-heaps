# TESTING FUNNEL HEAP
echo 'Funnel - 5050'
./main -v 1 -h funnel -t 1 -s 5050 -p 338350

echo 'Funnel - 63000'
./main -v 1 -h funnel -t 1 -s 63000 -p 10542250

echo 'Funnel - 125250'
